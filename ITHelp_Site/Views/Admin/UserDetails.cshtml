@model ITHelp_Models.User
@{ViewBag.Title = "User Details";}
@section Styles {
    <link href="@Url.Content("~/Content/Detail.css")" rel="stylesheet" type="text/css" />
}
<div style="text-align: center;">
    <h2 class="detailHeader">
    <div class="L1">
        Showing User: @Html.DisplayFor(model => model.Name)
        <br />
        User Name: @Html.DisplayFor(model => model.User_Name)
    </div>
</h2>
    <dl class="dl-horizontal" style="text-align: center;">
        <dt>
            @Html.DisplayNameFor(model => model.Enabled)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Enabled)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Admin)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Admin)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.User_Group)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.User_Group)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Phone_No)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Phone_No)
        </dd>

    </dl>
</div>
<p style="text-align: center;">
    @Html.ActionLink("Edit", "users/Edit", new { id = Model.Id }, new { @class = "btn btn_edit" })
    @Html.ActionLink("Back to List", "users", null, new { @class = "btn btn_back" })
</p>
